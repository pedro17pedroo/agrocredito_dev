{
  "name": "agrocredito",
  "version": "1.0.0",
  "description": "Sistema de Gestão de Crédito Agrícola para Angola",
  "type": "module",
  "license": "MIT",
  "author": "Equipa AgroCrédito",
  "keywords": [
    "agrocredito",
    "agricultura",
    "credito",
    "angola",
    "fintech"
  ],
  "scripts": {
    "dev": "cross-env NODE_ENV=development tsx server/index.ts",
    "dev:win": "set NODE_ENV=development && tsx server/index.ts",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "start:win": "set NODE_ENV=production && node dist/index.js",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "db:seed": "tsx scripts/seed.ts",
    "db:seed:win": "tsx scripts/seed.ts",
    "db:seed-credit-programs": "tsx scripts/seed-credit-programs.ts",
    "db:seed-credit-programs:win": "tsx scripts/seed-credit-programs.ts",
    "db:seed-applications": "tsx scripts/seed-applications.ts",
    "db:seed-applications:win": "tsx scripts/seed-applications.ts",
    "db:seed-all": "tsx server/run-all-seeds.ts",
    "db:seed-all:win": "tsx server/run-all-seeds.ts",
    "restore:win": "tsx scripts/restore-windows.ts",
    "restore:win:force": "tsx scripts/restore-windows.ts --force",
    "restore:win:v2": "tsx scripts/restore-windows-v2.ts",
    "restore:win:v2:force": "tsx scripts/restore-windows-v2.ts --force",
    "create:db:win": "tsx scripts/create-database-windows.ts",
    "backup:win": "tsx scripts/backup-windows.ts",
    "setup": "node scripts/setup.js",
    "backup": "node scripts/backup.js",
    "restore": "node scripts/restore.js",
    "restore:local": "powershell -ExecutionPolicy Bypass -File scripts/restore-local.ps1",
    "deploy": "node scripts/deploy.js",
    "lint": "echo Linting não configurado ainda",
    "clean": "rimraf dist node_modules/.cache",
    "health": "node -e \"const http = require('http'); const req = http.get('http://localhost:5000/api/health', (res) => { console.log('Servidor está a responder'); process.exit(0); }); req.on('error', () => { console.log('Servidor não está a responder'); process.exit(1); });\""
  },
